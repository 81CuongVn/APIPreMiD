{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ESNEXT",
		"inlineSourceMap": true,
		"rootDir": "./src",
		"outDir": "./dist",
		"removeComments": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"paths": {
			"*": ["./node_modules/*"]
		}
	},
	"include": ["./src/**/*"],
	"exclude": ["./devMode.ts"]
}
